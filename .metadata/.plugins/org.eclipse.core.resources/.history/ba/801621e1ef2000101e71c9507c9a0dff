package com.sj;

public class DIFramework {

	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	       DIFramework f1 = new DIFramework();
	       f1.injectDependency(args[0]);
	
	       
	      
	
	
	}
	
	Calculator createCalculator(String name) {
		
		if(name.equals("A"))
			return new CalculatorA();
		else
			return new CalculatorB();
		
	}
	
	public void injectDependency(String name) {
		User u  = new User();
		Calculator c = createCalculator(name);
		u.setCalc(c);
		u.invokeFindSqrrt(13);
	}

}
